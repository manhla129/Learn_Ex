    "import turtle\n",
    "tl = turtle.Turtle()\n",
    "\n",
    "d = int(input('Nhập d = '))\n",
    "i = 1\n",
    "d2 = 0\n",
    "while d2<d:\n",
    "    tl.circle(i*10,180)\n",
    "    d2 = turtle.distance(0,i*10)\n",
    "    i +=1\n",
    "turtle.done()\n"
   ]
  }
